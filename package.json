{
  "name": "workout-tracker",
  "version": "0.2.0",
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-polyfill": "^6.5.0",
    "classnames": "^2.2.3",
    "di-decorators": "^0.2.2",
    "hoodie": "git://github.com/hoodiehq/hoodie#tent",
    "hoodie-client": "^3.4.0",
    "joi": "^8.0.3",
    "purecss": "^0.6.0",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-autobind": "^1.0.6",
    "react-dom": "^0.14.7",
    "react-icons": "^1.0.3",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "recompose": "^0.15.0",
    "redux": "^3.3.1",
    "redux-thunk": "^1.0.3",
    "seamless-immutable": "^5.1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-strict-mode": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "browserify-global-shim": "^1.0.3",
    "cli-clear": "^1.0.4",
    "mobx-react-devtools": "^4.0.0",
    "onchange": "^2.0.0",
    "parallelshell": "^2.0.0",
    "postcss-cli": "^2.5.1",
    "postcss-import": "^8.0.2",
    "postcss-scss": "^0.1.3",
    "precss": "^1.4.0",
    "proxyquire": "^1.7.4",
    "redux-devtools": "^3.1.1",
    "reload": "^0.7.0",
    "simple-mock": "^0.6.0",
    "tap-min": "^1.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0",
    "tape-dom": "0.0.10",
    "watchify": "^3.7.0"
  },
  "engines": {
    "node": ">=4"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build": "parallelshell 'npm run build:js' 'npm run build:css'",
    "build:js": "browserify src/app.js -o www/assets/js/app.js",
    "build:css": "postcss -c postcss.conf.js -s postcss-scss -o www/assets/css/app.css css/app.css",
    "docker:couchdb": "docker run -it --rm -p 5984:5984 -v $PWD/couch:/usr/local/var/lib/couchdb klaemo/couchdb:latest",
    "hoodie": "hoodie",
    "start": "parallelshell 'npm run hoodie' 'npm run watch'",
    "sync": "browser-sync start --files ./www --proxy localhost:8080",
    "test": "BABEL_ENV=test babel-tape-runner tests/unit/**/*.js | tap-spec",
    "test:browser": "parallelshell 'npm run test:watchify' 'npm run test:serve'",
    "test:min": "node tests/clear.js && BABEL_ENV=test babel-tape-runner 'tests/unit/**/*.js' | tap-min",
    "test:watch": "onchange 'tests/**/*.js' 'src/**/*.js' -- npm run test:min --silent",
    "test:watchify": "watchify tests/browser.js tests/unit/**/*.js -o tests/tests.js",
    "test:serve": "reload -b -d ./tests -p 7001",
    "watch": "parallelshell 'npm run watch:js' 'npm run watch:css' 'npm run sync'",
    "watch:css": "onchange 'css/**/*.css' -- npm run build:css",
    "watch:js": "watchify src/app.js -o www/assets/js/app.js"
  }
}
