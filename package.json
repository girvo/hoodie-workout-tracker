{
  "name": "workout-tracker",
  "version": "0.2.0",
  "dependencies": {
    "babel-core": "^6.5.2",
    "classnames": "^2.2.3",
    "di-decorators": "^0.2.2",
    "hoodie": "git://github.com/hoodiehq/hoodie#tent",
    "hoodie-client": "^3.4.0",
    "joi": "^8.0.3",
    "mobx": "^2.0.0",
    "mobx-react": "^3.0.0",
    "purecss": "^0.6.0",
    "react": "^0.14.7",
    "react-autobind": "^1.0.6",
    "react-dom": "^0.14.7",
    "react-icons": "^1.0.3",
    "react-router": "^2.0.0",
    "recompose": "^0.15.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-tape-runner": "^2.0.1",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "browserify-global-shim": "^1.0.3",
    "cli-clear": "^1.0.4",
    "mobx-react-devtools": "^4.0.0",
    "postcss-cli": "^2.5.1",
    "postcss-import": "^8.0.2",
    "postcss-scss": "^0.1.3",
    "precss": "^1.4.0",
    "reload": "^0.7.0",
    "simple-mock": "^0.6.0",
    "tap-browser-el": "^2.0.0",
    "tap-min": "^1.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0",
    "tape-dom": "0.0.10",
    "watch": "^0.17.1",
    "watchify": "^3.7.0"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "hoodie": "hoodie",
    "start": "npm run hoodie & npm run watch",
    "build:css": "postcss -c postcss.conf.js -s postcss-scss -o www/assets/css/app.css css/app.css",
    "build:js": "browserify src/app.js -o www/assets/js/app.js -t [ babelify ]",
    "watch:js": "watchify src/app.js -o www/assets/js/app.js -t [ babelify ]",
    "watch:css": "watch 'npm run build:css' css/ --ignoreDotFiles --ignoreUnreadable",
    "sync": "browser-sync start --files ./www --proxy localhost:8080",
    "watch": "npm run watch:js & npm run watch:css & npm run sync",
    "test": "BABEL_ENV=test babel-tape-runner tests/unit/**/*.js* | tap-spec",
    "test:browser": "rm -f tests/tests.js && reload -b -d tests -p 7001 & watchify tests/browser.js tests/unit/**/*.js tests/unit/**/*.jsx -o tests/tests.js -t [ babelify ]",
    "test:watch": "watch 'node tests/clear.js && BABEL_ENV=test babel-tape-runner \"tests/unit/**/*.js*\" | node_modules/.bin/tap-min' tests/ src/ --ignoreDotFiles --ignoreUnreadable"
  }
}
