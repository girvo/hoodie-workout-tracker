{
  "name": "workout-tracker",
  "version": "0.1.0",
  "bundleDependencies": [
    "hoodie-plugin-appconfig",
    "hoodie-plugin-email",
    "hoodie-plugin-users",
    "hoodie-server"
  ],
  "dependencies": {
    "babel-core": "^6.5.2",
    "classnames": "^2.2.3",
    "di-decorators": "^0.2.2",
    "hoodie-client": "^3.4.0",
    "hoodie-plugin-appconfig": "^2.0.1",
    "hoodie-plugin-email": "^1.0.0",
    "hoodie-plugin-users": "^2.2.2",
    "hoodie-server": "^4.0.2",
    "mobx": "^2.0.0",
    "mobx-react": "^3.0.0",
    "purecss": "^0.6.0",
    "react": "^0.14.7",
    "react-autobind": "^1.0.6",
    "react-dom": "^0.14.7",
    "react-icons": "^1.0.3",
    "react-router": "^2.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "browserify-global-shim": "^1.0.3",
    "mobx-react-devtools": "^4.0.0",
    "postcss-cli": "^2.5.1",
    "postcss-import": "^8.0.2",
    "postcss-scss": "^0.1.3",
    "precss": "^1.4.0",
    "watchify": "^3.7.0"
  },
  "engines": {
    "node": ">=0.10.22"
  },
  "files": [
    "www/"
  ],
  "hoodie": {
    "plugins": [
      "hoodie-plugin-appconfig",
      "hoodie-plugin-email",
      "hoodie-plugin-users"
    ]
  },
  "scripts": {
    "hoodie": "node node_modules/hoodie-server/bin/start",
    "start": "npm run hoodie & npm run watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "postcss -c postcss.conf.js -s postcss-scss -o www/assets/css/app.css css/app.css",
    "build:js": "browserify src/app.js -o www/assets/js/app.js -t [ babelify ]",
    "watch:js": "watchify src/app.js -o www/assets/js/app.js -t [ babelify ]",
    "watch:css": "postcss -c postcss.conf.js -s postcss-scss -o www/assets/css/app.css --watch css/**/*.css css/app.css",
    "sync": "browser-sync start --files ./www --proxy localhost:6004",
    "watch": "npm run watch:js & npm run watch:css & npm run sync"
  },
  "type": "app",
  "readme": "ERROR: No README data found!"
}
